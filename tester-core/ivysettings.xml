<ivysettings>
  <settings defaultResolver="chain-example"/>
  
 <resolvers>
    <chain name="chain-example">
      <ibiblio name="ibiblio" m2compatible="true"/>
      <url name="codehaus">
  		<artifact pattern="http://repository.codehaus.org/[organisation]/[module]/[revision]/[artifact]-[revision].[ext]"/>
	  </url>
      <url name="jboss">
  		<artifact pattern="http://repository.jboss.com/maven2/[organisation]/[module]/[revision]/[artifact]-[revision].[ext]"/>
	  </url>
	  <url name="openqa">
  		<artifact pattern="http://archiva.openqa.org/repository/releases/[organisation]/[module]/[revision]/[artifact]-[revision].[ext]"/>
		</url>
    </chain>
    </resolvers>
</ivysettings>
